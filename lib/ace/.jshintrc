{
	/* env */
	"es3": true,
	"browser": true,

	/* lax jshint conventions */
	"asi": true,
	"boss": true,
	"debug": true,
	"eqnull": true,
	"evil": true,
	"expr": true,
	"funcscope": true,
	"iterator": true,
	"lastsemic": true,
	"laxbreak": true,
	"laxcomma" : true,
	"loopfunc": true,
	"maxerr": true,
	"multistr": true,
	"notypeof": true,
	"proto": true,
	"scripturl": true,
